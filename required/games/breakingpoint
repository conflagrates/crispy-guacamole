-- // Toggle Vars
_G.esp = true
_G.unban = true

local unbanLabel1 = trdMain:Label("click and join on a trade-banned")

local tutacc = accessories:Label("say !join me after clicking")
local tutknife = knives:Label("say !join me after clicking")
