-- // Library

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostDuckyy/UI-Libraries/main/Neverlose/source.lua"))()

-- // Variables
local vim = game:GetService("VirtualInputManager")
local uis = game:GetService("UserInputService")

-- // Menu
local Window = Library:Window({
    text = "Yor's"
})

local Credits1 = Window:TabSection({
    text = "lv#7305"
})

local Main = Credits1:Tab({
    text = "Main",
    icon = "rbxassetid://7999345313",
})

local MainSection = Main:Section({
    text = "Efficiency"
})

-- // Toggle Variables
_G.autoBlock = nil

-- // Toggle Functions
function autoBlock()
        -- // Virtual Input Manager, SendKeyEvent
        vim:SendKeyEvent(true, "Q", false, game)
        wait(0.000000000001)
        vim:SendKeyEvent(true, "E", false, game)
        wait(0.000000000001)
        -- // Q , E
end


MainSection:Toggle({
    text = "Auto-Block",
    state = false,
    callback = function(boolean)
        -- // Basic Toggle
        if state == false then
        boolean = true
        _G.autoBlock = true
        else
        boolean = false
        _G.autoBlock = false
        end
        -- // Toggle Function
        while _G.autoBlock do
        autoBlock()
        end
    end
})

-- // Guardian
loadstring(game:HttpGet("https://raw.githubusercontent.com/euronetic/crispy-guacamole/main/required/guardian.lua"))()
